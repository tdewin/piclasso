//pre
boxwid = 1
boxht = 1
margin = 0
$grey = 0x505861
$lightgrey = 0xf0f0f0
$purple = 0x8e71f4
$green = 0x00d15f
color = white
thickness = 0.02

boxrad = 0.15

fontscale = 2
$adj=-0.1

//beginsplit rect
B:box  fill $green color -1 thickness 0
text with .c at B+(0,$adj) "V" bold big

move to B.e+(0.1,0)

//beginsplit vbr
B:box  fill $green color -1 thickness 0
[
raw "M 15,45 h 30 v -30 m 0,30 l -25,-25 M 55,85 h 30 v -30 m 0,30 l -25,-25 "
box rad 0.03 ht 0.3 wid 0.3 with .sw at previous raw.c+(0.05,0.05)
box rad 0.03 ht 0.3 wid 0.3 with .ne at previous raw.c-(0.05,0.05) 
] with .c at B

move to B.e+(0.1,0)

//beginsplit proxy
B:box  fill $green color -1 thickness 0
raw with .c at B "M 30,20 h 20 v 20 m 0,-20 l -30,30M 70,80 h -20 v -20 m 0,20 l 30,-30" 

move to B.e+(0.1,0)

//beginsplit backup-repository,repo
B:box  fill $green color -1 thickness 0
[
box color white thickness 0.02  wid 0.2 ht 0.5 rad 0.07
box color white thickness 0.02  wid 0.1 ht 0.15 rad 0.03 with .sw at previous .se+(0.1,0)
box color white thickness 0.02  wid 0.1 ht 0.15 rad 0.03 with .sw at previous .se+(0.1,0)
box color white thickness 0.02  wid 0.1 ht 0.15 rad 0.03 with .sw at previous .se+(0.1,0)
] with .c at B.c

move to B.e+(0.1,0)
//beginsplit vault
B:box  fill $green color -1 thickness 0
[
  circle ht 0.2 fill white color -1
  raw at previous "M 30,30 l 40,40 M 70,30 l -40,40" color white
]  with .c at B.c

move to B.e+(0.1,0)

//beginsplit data-mover
B:box rad 0.5 fill $green color -1 thickness 0 ht 0.5 wid 0.5
raw with .c at B "M 30,20 h 20 v 20 m 0,-20 l -30,30 M 70,80 h -20 v -20 m 0,20 l 30,-30"   ht 0.5 wid 0.5

move to B.e+(0.1,0)

//beginsplit veeam-agent
B:box  fill $green color -1 thickness 0
text with .c at B+(0,$adj) "Agent" bold big

move to B.e+(0.1,0)

//beginsplit vm
B:box fill $grey color -1 thickness 0
text with .c at B+(0,$adj) "VM" bold big

move to B.e+(0.1,0)

//beginsplit vm-with-a-snapshot
B:box fill $grey color -1 thickness 0
text with .c at B+(0,$adj) "VM" bold big 
raw with .c at B "M 70,10 h20 v20 z" fill white

move to B.e+(0.1,0)

//beginsplit object-storage
B:box fill $green color -1 thickness 0
raw with .c at B "m27 20h46a6 6 50 0 1 6 7l-8 47a8 8 140 0 1-8 7h-26a8 8 40 0 1-8-7l-8-47a6 6 130 0 1 6-7z" 
[
box color white thickness 0.02  wid 0.1 ht 0.2 rad 0.03
box color white thickness 0.02  wid 0.05 ht 0.10 with .sw at previous .se+(0.05,0)
box color white thickness 0.02  wid 0.05 ht 0.10  with .sw at previous .se+(0.05,0)
box color white thickness 0.02  wid 0.05 ht 0.10 with .sw at previous .se+(0.05,0)
] with .c at B.c
raw with .c at B "m44 20 s -13 32-24 27c-6-3 2-21 2-21"

move to B.e+(0.1,0)

//beginsplit server
B:box fill $grey wid 3 rad 0.2
circle with .w at B.w+(0.1,0) fill white thickness 0 rad 0.1
line from previous.e+(0.1,0) to B.e-(0.1,0) thickness 0.03

move to B.e+(0.1,0)

//beginsplit triangle-ns
B: raw "M 0,0 H 100 L 50,100 z" wid 1 ht 1 rad 0.2 fill $lightgrey color $lightgrey

move to B.e+(0.1,0)

//beginsplit triangle-sn
B: raw "M 0,100 H 100 L 50,0 z" wid 1 ht 1 rad 0.2 fill $lightgrey color $lightgrey

move to B.e+(0.1,0)

//beginsplit triangle-ew
B: raw "M 0,0 V 100 L 100,50 z" wid 1 ht 1 rad 0.2 fill $lightgrey color $lightgrey

move to B.e+(0.1,0)

//beginsplit triangle-we
B: raw "M 100,0 V 100 L 0,50 z" wid 1 ht 1 rad 0.2 fill $lightgrey color $lightgrey


//beginsplit vsphere
HY: [
B  : raw "M 0,0 H 100 L 50,100 z" wid 2.8 ht 1 rad 0.2 fill $lightgrey color $lightgrey
H  : box with .s at B.n-(0,0.04) ht 1 wid 3 fill $green color $green rad 0.1 
S:   box fill $grey wid 1.5 ht 0.5 rad 0.15  with .n at B.s+(0,0.1)
circle with .w at S.w+(0.1,0) fill white thickness 0 rad 0.1
line from previous.e+(0.1,0) to S.e-(0.1,0) thickness 0.03 color white
T  : text "vSphere" bold big big big color white  with .c at H.c+(0,$adj)
]

